{
  "name": "css-color-name",
  "main": "./lib/main",
  "version": "0.4.0",
  "description": "Autocomplete of css predefined color names",
  "keywords": [
    "css",
    "color",
    "autocomplete",
    "named colors"
  ],
  "repository": "https://github.com/adarrra/css-color-name",
  "license": "MIT",
  "author": {
    "name": "Adara Belaya"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [
    "language-css:grammar-used",
    "language-less:grammar-used",
    "language-sass:grammar-used",
    "language-scss:grammar-used",
    "Stylus:grammar-used"
  ],
  "devDependencies": {
    "eslint": "^3.10.2",
    "eslint-config-es2015": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jasmine": "^2.2.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getProvider"
      }
    }
  }
}
